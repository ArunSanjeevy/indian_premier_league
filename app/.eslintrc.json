{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "indent": [
            2,
            2,
            {"SwitchCase": 1}
        ],
        "no-bitwise": [
            2
        ],
        "comma-dangle": [2, "never"],
        "comma-spacing": [2, {"before": false, "after": true}]
        }
}
